import java.io.*;
import java.util.*;

public class readFile{
	public static void main(String args[]) throws Exception {
		File f= new File("C:\\Users\\Rashon\\Documents\\study\\material_science.txt");
		FileReader st= new FileReader(f);
		char[]read = null;
		while(st.read()!=-1) {
			st.read(read);
		}
		System.out.print(read);
		
	}
}