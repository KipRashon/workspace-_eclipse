import java.io.*;
import java.util.*;

public class readFile{
	public static void main(String args[]) throws Exception {
		File f= new File("C:\\Users\\Rashon\\Documents\\study\\material_science.txt");
		FileReader st= new FileReader(f);
		while(st.read()!=-1) {
			System.out.println(st.read());
		}
		
	}
}