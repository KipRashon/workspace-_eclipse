package testproject;



class FreshJuice{
	enum FreshJuiceSize{MONDAY,TUESDAY,WEDNESDAY,THURSDAY,FRIDAY,SATURDAY,SUNDAY};
	FreshJuiceSize size;
}
public class Juice {
	public static void main(String args[])
	{
		FreshJuice juice= new FreshJuice();
		juice.size=FreshJuice.FreshJuiceSize.WEDNESDAY;
		System.out.println(juice.size);
	}
}
